<!doctype html>
<html>

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">

  <title>ITSM</title>
</head>

<body>
  <%- include('nav.html') %>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">API LINK</th>
        <th scope="col">설명</th>
        <th scope="col">항목</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope="row">1</th>
        <td><a href="http://localhost:5000/service"> Service </a></td>
        <td>서비스요청 리스트</td>
        <td>id(주요키), sr_id, type, title, contents, name, worker, state, startday, endday, ci_service, ci_asset01, ci_asset02, rel_event, rel_contents, rel_state</td>
      </tr>
      <tr>
        <th scope="row">2</th>
        <td><a href="http://localhost:5000/incident"> Incident</a></td>
        <td>장애요청 리스트</td>
        <td>id(주요키), sr_id, type, title, contents, name, worker, state, startday, endday, ci_service, ci_asset01, ci_asset02, rel_event, rel_contents, rel_state</td>
      </tr>
      <tr>
        <th scope="row">3</th>
        <td><a href="http://localhost:5000/metric"> Metric</a></td>
        <td>cpu/mem/disk 사용율</td>
        <td>id(주요키), labels, datasets : { type, label['','','','',''...](라벨없음처리), borderColor, data[] }</td>
      </tr>
      <tr>
        <th scope="row">4</th>
        <td><a href="http://localhost:5000/portalboard"> portalboard</a></td>
        <td>공지사항</td>
        <td>id(주요키), title, contents, name </td>
      </tr>
      <tr>
        <th scope="row">5</th>
        <td><a href="http://localhost:5000/chart"> chart</a></td>
        <td>장애현황</td>
        <td>id(주요키), labels[인프라장비], datasets[type, label, backgroundColor, data] </td>
      </tr>
    </tbody>
  </table>
   
